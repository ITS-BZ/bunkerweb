[rocky8-base]
name = Rocky Linux 8 Base OS
baseurl = https://dl.rockylinux.org/pub/rocky/8/BaseOS/%ARCH%/os/
gpgcheck = 1
enabled = 1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-Rocky-8

[rocky8-devel]
name = Rocky Linux 8 Devel
baseurl = https://dl.rockylinux.org/pub/rocky/8/Devel/%ARCH%/os/
gpgcheck = 1
enabled = 1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-Rocky-8
