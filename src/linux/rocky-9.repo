[rocky9-base]
name = Rocky Linux 9 Base OS
baseurl = https://dl.rockylinux.org/pub/rocky/9/BaseOS/%ARCH%/os/
gpgcheck = 1
enabled = 1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-Rocky-9

[rocky9-devel]
name = Rocky Linux 9 Devel
baseurl = https://dl.rockylinux.org/pub/rocky/9/devel/%ARCH%/os/
gpgcheck = 1
enabled = 1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-Rocky-9
